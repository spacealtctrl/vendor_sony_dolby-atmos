# Dolby data directory - use vendor_data_file (not custom type)
/data/vendor/dolby(/.*)?                                  u:object_r:vendor_data_file:s0

# Dolby libraries
/(vendor|system/vendor)/lib64/soundfx/libswdap\.so       u:object_r:same_process_hal_file:s0
